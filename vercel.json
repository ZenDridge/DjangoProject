{
    "builds": [
        {
            "src": "ICpEPHUB/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "150mb", 
                "runtime": "python3.9",
                "buildCommand": "bash setup.sh" 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "ICpEPHUB/wsgi.py"
        },
        {   "src": "/static/(.*)",
            "dest": "/static/$1" 
        }
    ]
}
